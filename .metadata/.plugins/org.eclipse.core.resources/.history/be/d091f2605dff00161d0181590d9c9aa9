
public class Coche {
	private String matricula;
	private String marca;
	private float precio;
	private Motor motor;
	
	//Constructor
	
	//Por defecto
	public Coche(){
		
	}
	
	//Constructor con todos los datos de un coche
	
	public Coche(String m, String marca,float precio,int nivelAceite,int cilindrada,int potecia){
		matricula = m;
		this.marca = marca;
		this.precio = precio;
		motor.setCilindrada(cilindrada);
		motor.setNivelAceite(nivelAceite);
	
	}
	
	
	
}
