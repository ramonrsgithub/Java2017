
public class SubFusil {
	private int municion;
	private String modelo;	
	private int precision;
	
	/************* Constructores *******************/
	public SubFusil(){
		
	}
	public SubFusil(int municion, String modelo, int precision) {
		this.municion = municion;
		this.modelo = modelo;
		this.precision = precision;
	}
	
	
}
